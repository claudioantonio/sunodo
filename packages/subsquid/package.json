        "viem": "^1.4"
    },
    "devDependencies": {
        "@subsquid/evm-typegen": "^3.2",
        "@subsquid/typeorm-codegen": "^1.2",
        "@sunodo/wagmi-plugin-hardhat-deploy": "*",
        "@types/node": "^20",
        "@typescript-eslint/eslint-plugin": "^5",
